{
  "tasks": {
    "dev": "deno run -A tools/dev.ts",
    "test": "deno test -A --no-lock ./test/unit && cd test/fixture && deno test --no-lock -A && cd ../.. && deno run -A tools/test-examples.ts basic with-csr with-unocss",
    "version-bump:minor": "deno run -A tools/patch.ts --release=minor",
    "version-bump:patch": "deno run -A tools/patch.ts --release=patch"
  },
  "lint": {
    "files": {
      "exclude": [
        ".deno-cache"
      ]
    }
  },
  "fmt": {
    "files": {
      "exclude": [
        ".deno-cache"
      ]
    },
    "options": {
      "indentWidth": 2,
      "useTabs": false,
      "singleQuote": false
    }
  },
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "react",
    "lib": [
      "deno.ns",
      "dom",
      "dom.iterable",
      "dom.asynciterable"
    ]
  },
  "importMap": "./importMap.dev.json"
}
