{
  "tasks": {
    "test": "cd test/fixture && deno test --allow-all",
    "test:watch": "cd test/fixture && deno test --allow-all --watch"
  },
  "fmt": {
    "options": {
      "indentWidth": 2,
      "useTabs": false,
      "singleQuote": false
    }
  },
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "react",
    "lib": [
      "deno.ns",
      "dom"
    ]
  },
  "importMap": "./importMap.dev.json"
}
